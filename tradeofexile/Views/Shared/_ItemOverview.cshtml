<table class="item-overview">
    <thead class="">
        <tr>
            <th>

            </th>
            <th class="th-name">
                Name
            </th>
            <th>
            </th>
            <th class="th-value">
                Value
            </th>
            <th class="th-seven-days-change">
                7 days change
            </th>
            <th class="th-occurances">
                Occurances
            </th>
        </tr>
    </thead>
    <tbody class="item-overview-tbody">
        @foreach (var item in Model.ItemsToShow)
        {
            <tr class="item-tr">
                <td class="item-icon"><img class="item-img" src="@item.IconLink." /></td>
                <td class="item-name">@item.Name</td>
                <td class="item-price-ammount">@item.Price.Ammount</td>
                <td class="item-price-type"> @item.Price.CurrencyType</td>
                <td class="item-seven-days-change">@item.SevenDaysChange</td>
                <td class="item-occurances">@item.Occurances</td>
            </tr>
        }
    </tbody>
</table>